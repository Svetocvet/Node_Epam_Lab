{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "max-len": ["error", { "code": 175 }],
        "indent": ["error", 4],
        "new-cap": 0
    }
}
